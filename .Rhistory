list_16 <- read.csv("data/2016.csv")
list_16 %>%
select(Country, Region, Happiness_Score, Economy, Health, Freedom, Trust, Dystopia_Residual)
table <- list_16 %>%
select(Region, Happiness_Score, Freedom)
scatter_plot <- ggplot(table, aes(x = Freedom, y = Happiness_Score)) +
geom_point() +
ggtitle("Level of Freedom and its Relation to Happiness")
scatter_plot
scatter_plot2 <- ggplot(table, aes(x = Freedom, y = Happiness_Score, color = Region)) +
geom_point() +
facet_wrap(~Region)+
geom_smooth(method=lm, se=FALSE) +
ggtitle("Level of Freedom and its Relation to Happiness by Region")
scatter_plot2
